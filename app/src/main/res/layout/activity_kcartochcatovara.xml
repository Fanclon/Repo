<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".kcartochcatovara">

    <LinearLayout
        android:layout_width="405dp"
        android:layout_height="564dp"
        android:layout_marginTop="164dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </LinearLayout>

    <TextView
        android:id="@+id/textView85"
        android:layout_width="378dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_weight="3"
        android:text="Отель гранд-будапешт"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        tools:layout_editor_absoluteY="178dp" />

    <TextView
        android:id="@+id/textView89"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Описание"
        tools:layout_editor_absoluteX="26dp"
        tools:layout_editor_absoluteY="248dp" />

    <TextView
        android:id="@+id/textView90"
        android:layout_width="335dp"
        android:layout_height="260dp"
        android:text="В самом сердце Стокгольма, где встречаются королевская набережная и дыхание Балтики, расположился Grand Waterfront Residence — легендарный символ шведской элегантности уже более 150 лет. Панорамные окна номеров открывают завораживающий вид на Королевский дворец, Старый город Гамла Стан и мерцающую гладь залива. Мраморные холлы, хрустальные люстры, сдержанная роскошь скандинавского шика и легендарный сервис, которому доверяют нобелевские лауреаты и мировые знаменитости."
        android:textAlignment="viewStart"
        android:textSize="15sp"
        tools:layout_editor_absoluteX="26dp"
        tools:layout_editor_absoluteY="274dp" />

    <TextView
        android:id="@+id/textView91"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Удобства"
        tools:layout_editor_absoluteX="34dp"
        tools:layout_editor_absoluteY="581dp" />

    <TextView
        android:id="@+id/textView92"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Wi-Fi"
        tools:layout_editor_absoluteX="34dp"
        tools:layout_editor_absoluteY="610dp" />

    <TextView
        android:id="@+id/textView93"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Spa"
        tools:layout_editor_absoluteX="82dp"
        tools:layout_editor_absoluteY="610dp" />

    <TextView
        android:id="@+id/textView94"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Парковка"
        tools:layout_editor_absoluteX="124dp"
        tools:layout_editor_absoluteY="610dp" />

    <ImageButton
        android:id="@+id/imageButton6"
        android:layout_width="335dp"
        android:layout_height="56dp"
        android:layout_marginBottom="32dp"
        app:layout_constraintBottom_toBottomOf="parent"
